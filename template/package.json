{
  "name": "<%= projectName -%>",
  "version": "1.0.0",
  "description": "<%= description -%>",
  "private": true,
  "author": "<%= author -%>",
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start-test": "egg-scripts start --env=test --title=egg-server-<%= projectName -%> --port 3000",
    "start-prod": "egg-scripts start --env=prod --title=egg-server-<%= projectName -%> --port 3000",
    "stop": "egg-scripts stop --title=egg-server-<%= projectName -%>",
    "dev": "ENV_PATH=env/.envv  egg-bin dev --port 3000",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ets": "ets",
    "ci": "npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean",
    "new": "plop"
  },
  "dependencies": {
    "@alicloud/pop-core": "^1.7.9",
    "@types/bluebird": "^3.5.29",
    "@types/validator": "^10.11.3",
    "ali-oss": "^6.3.1",
    "axios": "^0.24.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "class-validator-helper": "^0.0.4",
    "cross-blob": "^1.2.0",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.10",
    "egg": "^3.17.5",
    "egg-cors": "^2.2.3",
    "egg-logrotator": "^3.1.0",
    "egg-redis": "^2.5.0",
    "egg-scripts": "^2.17.0",
    "egg-sequelize-ts": "^0.2.3-0",
    "images": "^3.2.3",
    "ioredis": "^4.14.1",
    "jsonwebtoken": "^8.5.1",
    "md5js": "^1.0.7",
    "monk": "^7.1.1",
    "mysql2": "^2.0.0",
    "qs": "^6.9.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^1.0.0",
    "tencentcloud-sdk-nodejs": "^3.0.110",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.0.4",
    "@types/co": "^4.6.2",
    "@types/ioredis": "^4.14.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.10.9",
    "@types/qs": "^6.9.0",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.16.4",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "egg-ts-helper": "^1.25.6",
    "module-alias": "^2.2.2",
    "plop": "^2.5.3",
    "tslib": "^1.9.0",
    "tslint": "^5.14.0",
    "tslint-config-egg": "^1.0.0",
    "typescript": "^3.7.2"
  },
  "_moduleAliases": {
    "@base": "app/base",
    "@lib": "app/lib",
    "@util": "app/util",
    "@schema": "app/schema"
  },
  "engines": {
    "node": ">=14.20.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "license": "MIT"
}
